{
    "manifest_version": 3,
    "name": "School times",
    "version": "1.0",
    "description": "Show when to switch classes",
    "permissions": ["activeTab", "storage", "http://*/*", "https://*/*"],
    "background": {
      "service_worker": "popup.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  }

  
